            <section>
                <!-- <div class="fig-container" data-file="worldmap.html"></div> -->
                <h3 style="margin-bottom:0px;">COVID-19 Pandemic Onset Test</h3>
                <h4>MAST: Mean-Agnostic Sequential Test</h4>
                <div id="worldmap" class="svg-container"></svg>
            </section>
            {{#countries}}
