<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <title>MAST: COVID-19 pandemic onset test</title>

    <link rel="stylesheet" href="reveal.js/dist/reset.css">
    <link rel="stylesheet" href="reveal.js/dist/reveal.css">
    <link rel="stylesheet" href="reveal.js/dist/theme/league.css" id="theme">
    <link rel="stylesheet" href="flag-icon-css/css/flag-icon.min.css">

    <!-- Theme used for syntax highlighted code -->
    <link rel="stylesheet" href="reveal.js/plugin/highlight/monokai.css" id="highlight-theme">
    <!-- <link rel="stylesheet" href="reveal.js/plugin/highlight/zenburn.css"> -->

    <script src="reveal.js/dist/reveal.js"></script>
    <script src="reveal.js/plugin/notes/notes.js"></script>
    <script src="reveal.js/plugin/markdown/markdown.js"></script>
    <script src="reveal.js/plugin/highlight/highlight.js"></script>

    <script src="reveal.js-plugins/menu/menu.js"></script>
    
    <!-- moment.js (required by chart.js) -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js" integrity="sha512-qTXRIMyZIFb8iQcfjXWCO8+M5Tbc38Qi5WzdPOYZHIlZpzBHG3L3by84BBBOiRGiEb7KKtAOAs5qYdUiZiQNNQ==" crossorigin="anonymous"></script>

    <!-- chart.js -->
    <!-- <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script> -->
    <script src="reveal.js-plugins/chart/Chart.min.js"></script>
    <script src="reveal.js-plugins/chart/plugin.js"></script>

    <!-- chhart.js pan/zoom -->
    <script src="https://cdn.jsdelivr.net/npm/hammerjs@2.0.8"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-zoom@0.7.7"></script>

    <!-- ribbon -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/github-fork-ribbon-css/0.2.3/gh-fork-ribbon.min.css" />

    <style>
        path {
          stroke: white;
          stroke-width: 0.25px;
          fill: grey;
        }
        path.enabled {
            /*fill: #4e79a7;*/
            /*fill: #71e9f4;*/
            fill: #53ADB5;
        }
        .svg-container {
            display: inline-block;
            position: relative;
            width: 100%;
            vertical-align: top;
            overflow: hidden;
        }
        .svg-content {
            display: inline-block;
            position: absolute;
            top: 0;
            left: 0;
        }
        .flag-icon-squared {
          border-radius: 50%;
        }
        .flag-icon-smaller {
          width: 1em;
        }
        .github-fork-ribbon::before{
          /* gray */
          background-color: #6d6d6d;
          
          /* red */
          /*background-color: #aa0000; */

          /* orange */
          /*background-color: #ff9900; */

          /* green */
          /*background-color: #007200;*/
        }
        .reveal .home-button {
            position: fixed;
            left: 60px;
            bottom: 30px;
            z-index: 30;
            font-size: 24px;
        }
    </style>
</head>
<body>
    <a class="github-fork-ribbon" href="https://arxiv.org" data-ribbon="Read the paper on arXiv" title="Read the paper on arXiv">Read the paper on arXiv</a>
    <div class="reveal">
        <div class="home-button">
          <a href="#/">
            <i class="fas fa-home"></i>
          </a>
        </div>
